<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="rishvi_pos.ControlButtons" t-inherit="point_of_sale.ControlButtons" t-inherit-mode="extension">
    <xpath expr="//t[@t-if='!props.showRemainingButtons || (ui.isSmall and props.showRemainingButtons)']"
           position="after">
        <button t-on-click="onClickPopup" class="btn btn-light btn-lg flex-shrink-0 ms-auto">
    <i class="fa fa-truck me-1" role="img" aria-label="Shipping Service" title="Shipping Service"/>
    Shipping Service
</button>

    </xpath>
</t>
</templates>
